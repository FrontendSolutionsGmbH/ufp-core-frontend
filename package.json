{
  "name": "ufp-core",
  "version": "0.2.3",
  "description": "UFP Core - Build frontend applications at ease",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.3.0"
  },
  "scripts": {
    "ufp-make": "node node_modules/ufp-core/ext/build/scripts/make",
    "ufp-install": "npm install && node node_modules/ufp-core/ext/Install",
    "ufp-install-new": "npm install ../../src_modules/ufp-root",
    "ufp-update": "node node_modules/ufp-core/ext/UpdatePackageJson",
    "ufp-update-scripts": "node node_modules/ufp-core/ext/UpdatePackageJsonScripts",
    "ufp-update-dependencies": "node node_modules/ufp-core/ext/UpdatePackageJsonDependencies",
    "ufp-start": "cross-env NODE_ENV=development node node_modules/ufp-core/ext/build/scripts/start",
    "ufp-start-install": "npm run ufp-install-new && npm run ufp-start",
    "ufp-compile": "cross-env NODE_ENV=production npm run ufp-compile:bare",
    "ufp-compile:dev": "cross-env NODE_ENV=development npm run ufp-compile:bare",
    "ufp-compile:bare": "node node_modules/ufp-core/ext/build/scripts/compile",
    "webpack": "node build/scripts/compile",
    "ufp-lint": "eslint src --ext .jsx,.js --config node_modules/ufp-core/src/.eslintrc",
    "ufp-lint:fix": "npm run ufp-lint -- --fix",
    "ufp-test": "cross-env NODE_ENV=test karma start node_modules/ufp-core/ext/presets/default/config/karma.config",
    "ufp-test:watch": "npm run ufp-test -- --watch",
    "prepublishOnly": "npm run compile",
    "prepublish": "npm run compile",
    "prepare": "echo prepare",
    "publish": "echo publish",
    "prepack": "echo prepack",
    "postpack": "echo postpack",
    "preinstall": "echo preinstall",
    "postinstall": "echo postinstall",
    "preversion": "echo postinstall",
    "dev": "babel-node src/index.js",
    "make": "node build/make",
    "lint": "eslint **/*.js* -c src/.eslintrc ",
    "lint:fix": "npm run lint -- --fix ",
    "compile": "babel -d lib/ src/ --copy-files",
    "test": "cross-env NODE_ENV=test karma start build/karma.config",
    "watch": "node bin/watchSrc"
  },
  "bin": {
    "ufp-make": "ext/build/scripts/make.js"
  },
  "repository": "https://github.com/FrontendSolutionsGmbH/ufp-core-frontend.git",
  "keywords": [
    "ufp",
    "ultimate frontend platform",
    "webpack",
    "rxjs",
    "redux"
  ],
  "author": "Frontend Solutions GmbH",
  "contributors": [
