    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^2.1.4",
    "circular-dependency-plugin": "^5.0.2",
    "cli-table3": "^0.5.1",
